{
  "name": "machinjiri/framework",
  "description": "Machinjiri is a lightweight, flexible PHP framework for rapid web development.",
  "type": "library",
  "license": "MIT",
  "version": "1.2.0",
  "authors": [
    {
      "name": "Precious Lyson",
      "email": "precious.lyson@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Mlangeni Group",
      "email": "mlangenigroupqm@gmail.com",
      "role": "Company"
    }
  ],
  "require": {
    "php": "^8.2",
    "phpmailer/phpmailer": "^7.0.1",
    "symfony/console": "^5.3",
    "symfony/filesystem": "^5.3",
    "symfony/process": "^5.3",
    "firebase/php-jwt": "^6.11",
    "fakerphp/faker" : "^1.24.1",
    "predis/predis" : "^3.3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Mlangeni\\Machinjiri\\Core\\": "src/Machinjiri/",
      "Mlangeni\\Machinjiri\\Components\\": "src/Components/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mlangeni\\Machinjiri\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "keywords": [
    "mlangeni",
    "machinjiri",
    "preciouslyson",
    "framework",
    "php",
    "lightweight",
    "web",
    "development"
  ]
}